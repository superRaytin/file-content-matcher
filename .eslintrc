{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "no-restricted-syntax": [0],
    "no-mixed-operators": [0],
    "no-underscore-dangle": [0],
    "no-plusplus": [0],
    "no-param-reassign": [0],
    "arrow-parens": [0],
    "class-methods-use-this": [0],
    "camelcase": [0],
    "consistent-return": [0],
    "max-len": [0],
    "object-curly-newline": [0],
    "prefer-destructuring": [0],
    "semi": [0]
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}